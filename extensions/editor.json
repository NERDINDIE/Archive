// package.json
{
  "name": "my-extension",
  "displayName": "My Extension",
  "description": "An example extension",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.50.0"
  },
  "main": "./extension.js",
  "activationEvents": ["onCommand:extension.helloWorld"],
  "contributes": {
    "commands": [
      {
        "command": "extension.helloWorld",
        "title": "Hello World"
      }
    ]
  }
}
